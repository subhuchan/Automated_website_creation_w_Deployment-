{
  "version": 2,
  "builds": [
    {
      "src": "backend/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/app/main.py"
    }
  ],
  "env": {
    "GITHUB_TOKEN": "@github_token",
    "GITHUB_USERNAME": "@github_username",
    "GEMINI_API_KEY": "@gemini_api_key",
    "USER_SECRET": "@user_secret"
  }
}
