{
  "name": "tds-app-builder",
  "version": "2.0.0",
  "description": "AI-Powered Application Builder with React Frontend and FastAPI Backend",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn app.main:app --reload",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build"
  },
  "keywords": [
    "ai",
    "app-builder",
    "react",
    "fastapi",
    "gemini",
    "github-pages"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
